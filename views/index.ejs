<%- include('header'); -%>
    <div class="box" id="heading">
      <h1><%= listTitle %></h1>
    </div>
    <div class="box">
      <%  for (var i = 0; i < newLists.length; i++) { %>
        <div class="item">
          <input type="checkbox">
            <p><%= newLists[i] %></p>
        </div>
      <%  }  %>
        <!-- this post the infromation to my rooe server -->
        <form class="item" action="/" method="post">
          <input type="text" name="newList" placeholder="New item" autocomplete="off">
          <button type="submit" name="list" value="<%= listTitle %>">+</button>
        </form>

    </div>
  
<%- include('footer'); -%>
